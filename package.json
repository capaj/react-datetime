{
  "name": "@capaj/react-datetime",
  "version": "2.16.0",
  "description": "A lightweight but complete datetime picker React.js component",
  "homepage": "https://github.com/capaj/react-datetime",
  "repository": {
    "type": "git",
    "url": "https://github.com/capaj/react-datetime"
  },
  "main": "dist/DateTime.js",
  "module": "dist/mjs//DateTime.js",
  "typings": "dist/DateTime.d.ts",
  "files": [
    "src",
    "css",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc || true && tsc -p tsconfig-esm.json || true",
    "prepublishOnly": "npm run build",
    "dev": "cd example && parcel index.html",
    "lint": "eslint src/**/*.tsx test/**/*",
    "notify-pre-commit-hook": "echo '### Starting pre-commit hook ðŸ¦„'",
    "test": "./node_modules/.bin/jest",
    "test:snapshot": "jest snapshot",
    "test:snapshot:update": "./node_modules/.bin/jest snapshot --updateSnapshot",
    "test:all": "echo 'Running tests...' && npm run test && echo 'All tests passed! ðŸ¤˜'",
    "w": "jest --watch"
  },
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "timepicker",
    "datetimepicker",
    "datetime"
  ],
  "author": "Javier Marquez",
  "license": "MIT",
  "peerDependencies": {
    "moment": ">=2.24.0",
    "react": ">=0.13",
    "react-dom": ">=0.13",
    "moment-timezone": "^0.5.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/create-react-class": "^15.6.2",
    "@types/jest": "^24.0.11",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^1.3.1",
    "jest": "^24.7.0",
    "moment": ">=2.24.0",
    "moment-timezone": "^0.5.23",
    "parcel-bundler": "^1.12.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "proxy-date": "^0.1.1",
    "react": "^17.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^16.8.6",
    "request": "^2.88.0",
    "ts-jest": "^24.0.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@capaj/react-onclickoutside": "^6.7.1",
    "babel-plugin-transform-react-createelement-to-jsx": "^1.1.0",
    "create-react-class": "^15.6.3",
    "eslint-plugin-prettier": "^3.0.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2"
  },
  "pre-commit": [
    "notify-pre-commit-hook",
    "lint",
    "test:all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
